<div>
<% if (user.membership) { %>
    <div>
        <p><strong><%= msg.title %></strong></p>
        <p><%= msg.message %></p>
        <p><%= user.first_name %> <%= user.last_name %></p>
        <p><%= msg.time %></p>
    </div>

    <% if (user.isadmin) { %>
        <div>
            <button>Delete</button>
        </div>
    <%}%>
<%} else {%>
    <div>
        <p><strong><%= msg.title %></strong></p>
        <p><%= msg.message %></p>
        <p>Posted by: Anonymous</p>
        <p>Posted at: Secret time</p>
    </div>
<%}%>
</div>